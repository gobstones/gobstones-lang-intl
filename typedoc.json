{
    "entryPoints": [
        "./src"
    ],
    "out": "docs",
    "includes": "./src",
    "exclude": [
        "**/*.test.ts",
        "**/index.ts"
    ],
    "theme": "./node_modules/typedoc-neo-theme/bin/default",
    "includeVersion": true,
    "categorizeByGroup": true,
    "excludeExternals": true,
    "excludePrivate": false,
    "hideGenerator": true,
    "disableSources": true,
    "links": [
        {
            "label": "Repository",
            "url": "https://github.com/gobstones/gobstones-lang-intl"
        },
        {
            "label": "Gobstones Docs",
            "url": "https://dev.gobstones.org"
        },
        {
            "label": "Contribution Guidelines",
            "url": "https://dev.gobstones.org/contribution-guidelines"
        }
    ],

    "outline": [{
        "Public Modules": {
          "GobstonesTranslator": "models_GobstonesTranslator",
          "LocaleDefinition": "models_LocaleDefinition",
          "Errors": "models_Errors"
        },
        "Internal Modules": {
          "StringReader": "helpers_StringReader",
          "BidirectionalMap": "helpers_BidirectionalMap",
          "Translation": "translations_Locale",
          "Locale": "translations_Locale"
        }
      }]
}
